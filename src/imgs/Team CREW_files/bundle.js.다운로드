  const saveEditedCommentHandler = async commentId => {
    try {
      const updatedComment = {
        comment: modiyValue
      };
      renderTriggerHandler();
      const res = await shared_api__WEBPACK_IMPORTED_MODULE_3__["default"].correctionComment(boatId, commentId, updatedComment, config);
      alert(res.data.message);
      renderTriggerHandler();
      setIsModiy(null);
    } catch (err) {
      console.log(err);
    }
  };

  // 댓글 삭제 버튼
  const deleteCommentHandler = async commentId => {
    try {
      const deletedAt = new Date();
      const newDeleteData = {
        deletedAt
      };
      const res = await shared_api__WEBPACK_IMPORTED_MODULE_3__["default"].deleteComment(boatId, commentId, newDeleteData, config);
      alert(res.data.message);
      renderTriggerHandler();
    } catch (err) {
      console.log(err);
    }
  };

  // 수정 모달 취소 버튼
  const cancelEditCommentHandler = () => {
    setModiyValue(""); // 수정 중인 값을 초기화
    setIsModiy(null); // 수정 상태를 해제
  };

