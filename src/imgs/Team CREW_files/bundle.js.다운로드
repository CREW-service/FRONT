    if (e.id !== o.id) return !1;
    if (e.libraries.length !== o.libraries.length) return !1;
    for (let t = 0; t < e.libraries.length; ++t) if (e.libraries[t] !== o.libraries[t]) return !1;
    return e.nonce === o.nonce && e.retries === o.retries && e.url === o.url;
  }
}
