      postpone(function () {
        return _this.setEditorSelection(editor_1, selection_1);
      });
    }
  };
  ReactQuill.prototype.instantiateEditor = function () {
    if (this.editor) {
      this.hookEditor(this.editor);
    } else {
      this.editor = this.createEditor(this.getEditingArea(), this.getEditorConfig());
    }
  };
